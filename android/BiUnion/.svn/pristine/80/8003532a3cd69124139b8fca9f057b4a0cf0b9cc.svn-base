package com.bibinet.biunion.project.bean;import android.text.TextUtils;import com.bibinet.biunion.project.utils.PhoneNumberUtils;import com.bibinet.biunion.project.utils.ToastUtils;import java.io.Serializable;import java.util.List;/** * Created by bibinet on 2017-6-26. */public class RegisterBean implements Serializable {    private String company;    private String email;    private String name;    private String phone;    private String position;    private String code;    private String password;    public String getCompany() {        return company;    }    public void setCompany(String company) {        this.company = company;    }    public String getEmail() {        return email;    }    public void setEmail(String email) {        this.email = email;    }    public String getName() {        return name;    }    public void setName(String name) {        this.name = name;    }    public String getPhone() {        return phone;    }    public void setPhone(String phone) {        this.phone = phone;    }    public String getPosition() {        return position;    }    public void setPosition(String position) {        this.position = position;    }    public String getCode() {        return code;    }    public void setCode(String code) {        this.code = code;    }    public String getPassword() {        return password;    }    public void setPassword(String password) {        this.password = password;    }    public boolean checkInput() {        if (TextUtils.isEmpty(company)) {            ToastUtils.showShort("企业名称不能为空");            return false;        } else if (!PhoneNumberUtils.isEmail(email)) {            ToastUtils.showShort("邮箱格式不正确");            return false;        } else if (TextUtils.isEmpty(name)) {            ToastUtils.showShort("联系人不能为空");            return false;        } else if (TextUtils.isEmpty(position)) {            ToastUtils.showShort("职位不能为空");            return false;        } else if (!PhoneNumberUtils.isMobileNumber(phone)) {            ToastUtils.showShort("请输入正确的手机号");            return false;        } else if (TextUtils.isEmpty(code)) {            ToastUtils.showShort("验证码不能为空");            return false;        } else if (TextUtils.isEmpty(password)) {            ToastUtils.showShort("密码不能为空");            return false;        } else if (!PhoneNumberUtils.isPassword(password)) {            ToastUtils.showShort("密码格式不正确");            return false;        }        return true;    }}