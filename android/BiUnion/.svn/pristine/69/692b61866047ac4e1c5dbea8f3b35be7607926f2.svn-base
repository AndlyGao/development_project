<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dim_write_tender_book_height"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:gravity="center_vertical"
                    android:paddingLeft="13dp"
                    android:text="标书选择："
                    android:textColor="#333333"
                    android:textSize="13sp" />

                <LinearLayout
                    android:id="@+id/act_write_tender_book_select"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="4"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/act_write_tender_book_select_show"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="right|center_vertical"
                        android:hint="请选择标书"
                        android:paddingRight="13dp"
                        android:textColor="#999999"
                        android:textSize="13sp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dim_write_tender_book_height"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:gravity="center_vertical"
                    android:paddingLeft="13dp"
                    android:text="项目类型："
                    android:textColor="#333333"
                    android:textSize="13sp" />

                <LinearLayout
                    android:id="@+id/act_write_tender_book_project_type"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="4.5"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/act_write_tender_book_project_type_show"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="right|center_vertical"
                        android:hint="请选择项目类型"
                        android:paddingRight="13dp"
                        android:textColor="#999999"
                        android:textSize="13sp" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:background="#e2e3e6" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dim_write_tender_book_height"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:gravity="center_vertical"
                    android:paddingLeft="13dp"
                    android:text="招标方式："
                    android:textColor="#333333"
                    android:textSize="13sp" />

                <LinearLayout
                    android:id="@+id/act_write_tender_book_tender_style"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="4.5"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/act_write_tender_book_tender_style_show"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="right|center_vertical"
                        android:hint="请选择招标方式"
                        android:paddingRight="13dp"
                        android:textColor="#999999"
                        android:textSize="13sp" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:background="#e2e3e6" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dim_write_tender_book_height"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:gravity="center_vertical"
                    android:paddingLeft="13dp"
                    android:text="标书类型："
                    android:textColor="#333333"
                    android:textSize="13sp" />

                <LinearLayout
                    android:id="@+id/act_write_tender_book_type"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="4.5"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/act_write_tender_book_type_show"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="right|center_vertical"
                        android:hint="请选择标书的类型"
                        android:paddingRight="13dp"
                        android:textColor="#999999"
                        android:textSize="13sp" />

                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:background="#e2e3e6" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dim_write_tender_book_height"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:gravity="center_vertical"
                    android:paddingLeft="13dp"
                    android:text="联系人："
                    android:textColor="#333333"
                    android:textSize="13sp" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="4.5"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/act_write_tender_book_name"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@null"
                        android:gravity="right|center_vertical"
                        android:hint="请输入联系人姓名"
                        android:paddingRight="13dp"
                        android:singleLine="true"
                        android:textColor="#333333"
                        android:textSize="13sp" />


                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:background="#e2e3e6" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dim_write_tender_book_height"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:gravity="center_vertical"
                    android:paddingLeft="13dp"
                    android:text="联系方式："
                    android:textColor="#333333"
                    android:textSize="13sp" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="4.5"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/act_write_tender_book_phone"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:background="@null"
                        android:gravity="right|center_vertical"
                        android:hint="请输入联系方式"
                        android:inputType="number"
                        android:paddingRight="13dp"
                        android:singleLine="true"
                        android:textColor="#333333"
                        android:textSize="13sp" />


                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:background="#e2e3e6" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dim_write_tender_book_height"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:gravity="center_vertical"
                    android:paddingLeft="13dp"
                    android:text="联系邮箱："
                    android:textColor="#333333"
                    android:textSize="13sp" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="4.5"
                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/act_write_tender_book_email"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:background="@null"
                        android:gravity="right|center_vertical"
                        android:hint="请输入联系邮箱"
                        android:inputType="textEmailAddress"
                        android:paddingRight="13dp"
                        android:singleLine="true"
                        android:textColor="#333333"
                        android:textSize="13sp" />

                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:background="#e2e3e6" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="42dp"
                android:gravity="center_vertical"
                android:paddingLeft="13dp"
                android:text="备注"
                android:textColor="#333333"
                android:textSize="13sp" />

            <EditText
                android:layout_width="match_parent"
                android:layout_height="95dp"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:background="@drawable/shape_write_tender_book_background"
                android:gravity="start"
                android:hint="填写开标时间，个人需求....."
                android:inputType="textEmailAddress"
                android:paddingLeft="8dp"
                android:paddingRight="13dp"
                android:paddingTop="10dp"
                android:singleLine="true"
                android:textColor="#333333"
                android:textSize="13sp" />

            <Button
                android:id="@+id/act_write_tender_book_confirm"
                style="?android:attr/borderlessButtonStyle"
                android:layout_width="match_parent"
                android:layout_height="38dp"
                android:layout_marginLeft="11dp"
                android:layout_marginTop="23dp"
                android:layout_marginRight="11dp"
                android:background="@drawable/select_confirm_btn_background"
                android:text="提交"
                android:textColor="@color/white"
                android:textSize="14sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="14dp"
                    android:layout_height="14dp"
                    android:src="@mipmap/img_write_tender_book_position" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:text="www.bibenet.com"
                    android:textColor="#0c91fe"
                    android:textSize="12sp" />

                <ImageView
                    android:layout_width="14dp"
                    android:layout_height="14dp"
                    android:layout_marginLeft="15dp"
                    android:src="@mipmap/img_write_tender_book_call" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:text="400-000-0388"
                    android:textColor="#0c91fe"
                    android:textSize="12sp" />

                <ImageView
                    android:layout_width="14dp"
                    android:layout_height="14dp"
                    android:layout_marginLeft="15dp"
                    android:src="@mipmap/img_write_tender_book_phone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:text="13593156216"
                    android:textColor="#0c91fe"
                    android:textSize="12sp" />
            </LinearLayout>


        </LinearLayout>
    </ScrollView>
</LinearLayout>