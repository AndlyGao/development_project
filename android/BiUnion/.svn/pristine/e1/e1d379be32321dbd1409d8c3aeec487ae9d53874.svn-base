    public static String getBaseUrl(int type){
        String base = "";
        switch (NET_INTERFACE_TYPE){
            case NET_TYPE_FORMAL:
                if(type==TYPE_PIS){
                    base+=FORMAL_PIS;
                }else if(type==TYPE_IIP){
                    base+=FORMAL_IIP;
                }else{
                    base+=FORMAL_MSOP;
                }
                return base;
        }
        return base;
    }

    private static String getBaseUrl(){
        return FORMAL_VUE;
    }

