package com.bibinet.biunion.project.models;


import java.io.Serializable;

public class MediaVideoDetailTeacherModel implements Serializable {
    private String teacherName; //课程名称
    private String teacherHead; //图片
    private String teacherIntro; //介绍
    private String teacherIntroduction; //详细介绍
    private String code;

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }

    public String getTeacherName() {
        return teacherName;
    }

    public void setTeacherName(String teacherName) {
        this.teacherName = teacherName;
    }

    public String getTeacherHead() {
        return "review?filePath=" + teacherHead;
    }

    public void setTeacherHead(String teacherHead) {
        this.teacherHead = teacherHead;
    }

    public String getTeacherIntro() {
        return teacherIntro;
    }

    public void setTeacherIntro(String teacherIntro) {
        this.teacherIntro = teacherIntro;
    }

    public String getTeacherIntroduction() {
        return teacherIntroduction;
    }

    public void setTeacherIntroduction(String teacherIntroduction) {
        this.teacherIntroduction = teacherIntroduction;
    }
}