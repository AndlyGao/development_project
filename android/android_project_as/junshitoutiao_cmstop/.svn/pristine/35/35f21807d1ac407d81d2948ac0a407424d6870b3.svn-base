<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/item_channel_item_two_big_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:id="@+id/item_channel_item_list_two_big"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <RelativeLayout
            android:id="@+id/item_channel_item_list_two_big_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="?attr/common_item_bg_state_value" >

            <View
                android:id="@+id/gap_top_1"
                android:layout_width="0dp"
                android:layout_height="0dp" />

            <RelativeLayout
                android:id="@+id/item_list_news_bg_1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/gap_top_1"
                android:layout_marginBottom="8dp"
                android:layout_marginLeft="@dimen/common_horizontal_margin"
                android:layout_marginRight="6dp"
                android:layout_marginTop="@dimen/common_horizontal_margin" >

                <ImageView
                    android:id="@+id/item_channel_item_image_1"
                    android:layout_width="match_parent"
                    android:layout_height="124dp"
                    android:scaleType="centerCrop"
                    android:src="@color/transparent" />

                <TextView
                    android:id="@+id/item_channel_item_title_two_big_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/item_channel_item_image_1"
                    android:layout_marginBottom="2dp"
                    android:layout_marginTop="8dp"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/common_item_title_line_space"
                    android:maxLines="3"
                    android:textColor="?attr/item_title_color_value"
                    android:textSize="@dimen/channel_item_title_text_size" />

                <TextView
                    android:id="@+id/item_channel_item_tag_1"
                    style="@style/ChannelItemTag"
                    android:layout_below="@id/item_channel_item_title_two_big_1"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/item_channel_item_time_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/item_channel_item_title_two_big_1"
                    android:layout_toRightOf="@id/item_channel_item_tag"
                    android:textColor="?attr/item_summary_color_value"
                    android:textSize="@dimen/channel_item_time_text_size" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_below="@id/item_channel_item_title_two_big_1"
                    android:gravity="center"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/item_channel_item_comment_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:drawableLeft="@drawable/list_item_comment2"
                        android:drawablePadding="4dp"
                        android:text="11"
                        android:textColor="?attr/item_summary_color_value"
                        android:textSize="@dimen/channel_item_time_text_size" />

                    <TextView
                        android:id="@+id/item_channel_item_like_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="6dp"
                        android:drawableLeft="@drawable/list_item_zan"
                        android:drawablePadding="6dp"
                        android:text="11"
                        android:textColor="?attr/item_summary_color_value"
                        android:textSize="@dimen/channel_item_time_text_size"
                        android:visibility="gone" />
                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/item_channel_item_list_two_big_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="?attr/common_item_bg_state_value" >

            <View
                android:id="@+id/gap_top_2"
                android:layout_width="0dp"
                android:layout_height="0dp" />

            <RelativeLayout
                android:id="@+id/item_list_news_bg_2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/gap_top"
                android:layout_marginBottom="8dp"
                android:layout_marginLeft="6dp"
                android:layout_marginRight="@dimen/common_horizontal_margin"
                android:layout_marginTop="@dimen/common_horizontal_margin" >

                <ImageView
                    android:id="@+id/item_channel_item_image_2"
                    android:layout_width="match_parent"
                    android:layout_height="124dp"
                    android:scaleType="centerCrop"
                    android:src="@color/transparent" />

                <TextView
                    android:id="@+id/item_channel_item_title_two_big_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/item_channel_item_image_2"
                    android:layout_marginBottom="2dp"
                    android:layout_marginTop="8dp"
                    android:ellipsize="end"
                    android:lineSpacingMultiplier="@string/common_item_title_line_space"
                    android:maxLines="3"
                    android:textColor="?attr/item_title_color_value"
                    android:textSize="@dimen/channel_item_title_text_size" />

                <TextView
                    android:id="@+id/item_channel_item_tag_2"
                    style="@style/ChannelItemTag"
                    android:layout_below="@id/item_channel_item_title_two_big_2"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/item_channel_item_time_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/item_channel_item_title_two_big_2"
                    android:layout_toRightOf="@id/item_channel_item_tag"
                    android:textColor="?attr/item_summary_color_value"
                    android:textSize="@dimen/channel_item_time_text_size" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_below="@id/item_channel_item_title_two_big_2"
                    android:gravity="center"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/item_channel_item_comment_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:drawableLeft="@drawable/list_item_comment2"
                        android:drawablePadding="4dp"
                        android:text="11"
                        android:textColor="?attr/item_summary_color_value"
                        android:textSize="@dimen/channel_item_time_text_size" />

                    <TextView
                        android:id="@+id/item_channel_item_like_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="6dp"
                        android:drawableLeft="@drawable/list_item_zan"
                        android:drawablePadding="6dp"
                        android:text="11"
                        android:textColor="?attr/item_summary_color_value"
                        android:textSize="@dimen/channel_item_time_text_size"
                        android:visibility="gone" />
                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>
    </LinearLayout>

    <View
        android:id="@+id/item_channel_item_divider"
        android:layout_width="match_parent"
        android:layout_height="@dimen/channel_item_divider_height_common"
        android:layout_below="@+id/item_channel_item_list_two_big"
        android:layout_marginLeft="@dimen/common_horizontal_margin"
        android:layout_marginRight="@dimen/common_horizontal_margin"
        android:layout_marginTop="8dp"
        android:background="?attr/item_divider_color_value" />

</RelativeLayout>