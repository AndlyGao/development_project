<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="ArticleBottomLikeBtnLayout">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/article_btn_bg</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:paddingTop">4dp</item>
    </style>
    <style name="ArticleBottomBtnLayout">
        <item name="android:layout_width">68dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/article_btn_bg</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:paddingTop">4dp</item>
    </style>
    <style name="ArticleBottomBtnContent">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/article_bottom_btn_text</item>
        <item name="android:textSize">14dp</item>
    </style>

    <style name="SettingGroup">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">?attr/setting_group_bg_value</item>
        <item name="android:paddingLeft">@dimen/common_horizontal_margin</item>
        <item name="android:paddingRight">@dimen/common_horizontal_margin</item>
        <item name="android:paddingTop">@dimen/common_horizontal_margin</item>
        <item name="android:paddingBottom">@dimen/common_horizontal_margin</item>
        <item name="android:textColor">?attr/setting_group_text_value</item>
        <item name="android:textSize">@dimen/setting_group_text_size</item>
    </style>
    <style name="SettingGroup_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">?attr/setting_group_bg_value</item>
        <item name="android:padding">@dimen/common_horizontal_margin</item>
        <item name="android:textColor">?attr/setting_child_text_value</item>
        <item name="android:textSize">@dimen/setting_group_text_size</item>
    </style>

    <style name="SettingChildText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">@dimen/common_horizontal_margin</item>
        <item name="android:paddingRight">@dimen/common_horizontal_margin</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingBottom">12dp</item>
        <item name="android:textColor">?attr/setting_child_text_value</item>
        <item name="android:textSize">@dimen/setting_child_text_size</item>
    </style>

    <style name="SettingOfflineProgressText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">@dimen/common_horizontal_margin</item>
        <item name="android:paddingRight">@dimen/common_horizontal_margin</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingBottom">12dp</item>
        <item name="android:textColor">?attr/setting_offline_progress_text_value</item>
        <item name="android:textSize">12sp</item>
    </style>
    
    <style name="SettingDivider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2px</item>
        <item name="android:background">?attr/item_divider_color_value</item>
        <item name="android:layout_marginLeft">@dimen/common_horizontal_margin</item>
    </style>

    <!-- 下拉 转圈 -->
    <style name="ProgressBarStyleblue">

		<item name="android:indeterminateDrawable">@drawable/progress_blue</item>

		<item name="android:minWidth">22dip</item>

		<item name="android:maxWidth">22dip</item>

		<item name="android:minHeight">22dip</item>

		<item name="android:maxHeight">22dip</item>
    </style>
    
    <!--字体阴影效果  -->
    <style name="NameTextShadow">
        <item name="android:shadowColor">#4d4d4d</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">2</item>
        <item name="android:shadowRadius">1</item>
    </style>
    <!--圈子用户名字体阴影效果  -->
    <style name="CircleNameTextShadow">
        <item name="android:shadowColor">#212227</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">2</item>
        <item name="android:shadowRadius">1</item>
    </style>
    
    <style name="myContentTextStyle">  
        <item name="android:textColor">?attr/item_title_color_value</item>
        <item name="android:textSize">15sp</item>
    </style>
    
    <style name="myContentTextStyleGray">  
        <item name="android:textColor">?attr/article_time_value</item>
        <item name="android:textSize">15sp</item>  
    </style>
    
    <style name="myCompleteTextStyleRed">  
        <item name="android:textColor">@color/red</item>
        <item name="android:textSize">12sp</item>  
    </style>
    
    <style name="myContentTitleTextStyle">  
        <item name="android:textColor">?attr/item_title_color_value</item>
        <item name="android:textSize">18sp</item>  
    </style>
    
    <style name="myContentTextStyleGraySmall" parent="myContentTextStyleGray">  
        <item name="android:textSize">12sp</item>  
    </style>
    <style name="EditDialog" parent="android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowBackground">@color/transparent</item>
    	<item name="android:windowNoTitle">true</item>
    	<item name="android:windowIsFloating">true</item>
    </style>
    <!-- 标签按钮的样式 -->
    <style name="tabButton">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">?attr/toolbar_text_color_value</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="android:drawablePadding">0dp</item>
        <item name="android:lineSpacingExtra">4dp</item>
    </style>

