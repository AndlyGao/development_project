package com.chengning.yiqikantoutiao.event;

import com.chengning.yiqikantoutiao.Consts.ArticleType;

public class CollectEditStateEvent {
	private boolean isHasData;
	private boolean isEditFlag;
	private ArticleType type;

	public boolean isHasData() {
		return isHasData;
	}

	public void setHasData(boolean isHasData) {
		this.isHasData = isHasData;
	}

	public boolean isEditFlag() {
		return isEditFlag;
	}

	public void setEditFlag(boolean isEditFlag) {
		this.isEditFlag = isEditFlag;
	}

	public ArticleType getType() {
		return type;
	}

	public void setType(ArticleType type) {
		this.type = type;
	}
}
