
/**
 * 面包屑导航样式
 */
.breadcrumb-mqh-tit {
  text-align: left;
  font-weight: 600;
  padding: 24px;
  margin: 20px;
  background-color: #fff;
}

/**
 * 面包屑导航右侧-返回按钮
 */
.go-back-btn {
	position: absolute;
	right: 0;
	margin-right: 40px;
	margin-top: -25px;
}
/**
 * 搜素盒子-样式
 */
.search-box {
  margin: 20px;
  padding: 20px;
  background: #fff;
}
/**
 * 主体内容盒子-样式
 */
.maincont-box {
  margin: 20px 20px 130px;
  padding: 20px;
  background: #fff;
}

/**
 * tabs切换-选中样式
 */
.el-tabs__item.is-active {
  border-top: 2px solid;
}

/**
 * 弹窗-头部样式
 */
.el-dialog__header {
  border-bottom: 1px solid #eeeeee;
}

/**
  * 表格头部样式
 */
.el-table th {
  background-color: #f2f2f2;
}
.el-table .table-row-bg {
  background-color: #f2f2f2;
}






