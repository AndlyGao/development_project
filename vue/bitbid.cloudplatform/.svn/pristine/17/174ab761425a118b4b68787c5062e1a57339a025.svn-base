<script>
import {downloadFile} from '@/assets/js/common'
import {resultInfo} from '@/api/cloudplatform'
export default {
  data () {
    return {
      updateForm: {},
      tableData: [],
      rules: {}
    }
  },
  methods: {
    lookFile (file) {
      console.log(file)
      downloadFile(file.fileName, file.relativePath)
    },
    close () {
      this.$router.go(-1)
    },
    getResultInfo () {
      console.log('项目，标段信息未展示')
      resultInfo.getById(this.$route.params.objectId).then(res => {
        this.updateForm = res.data.resultInfo
        if (this.updateForm.content) {
          this.$refs.ueditor.setContent(this.updateForm.content)
        }
      })
    }
  },
  mounted () {
    this.getResultInfo()
  }
}
</script>
