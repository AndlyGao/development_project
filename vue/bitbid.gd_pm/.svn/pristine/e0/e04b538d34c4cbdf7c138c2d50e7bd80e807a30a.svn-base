}
.logo_box{
  height: 80px;
  background:#1b315a;
  line-height: 63px;
  border-bottom: 1px solid #294780;
  padding: 0 15px;
  box-sizing: border-box;
  position: relative;
}
.side_bigbox{
  overflow: hidden;
  position: relative;
}
.el-menu {
  border-right: solid 0px transparent;
  list-style: none;
  margin: 0;
  padding-left: 0;
  min-height: 10px!important;
  overflow: hidden;
}
.logo_box>img{
  position: absolute;
  top:0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}
.side_nav{
  border-bottom: 1px solid #294780;
}
#big_box .leftside_box{
  width: 12.5%;
  background-color: rgb(27, 49, 90);
}
.side_nav .el-submenu__title{
  background: #1b315a!important;
}
.side_nav .el-menu-item{
  background: #040e22!important;
}
.side_bigbox>ul{
  height: 100%;
  overflow-x: hidden;
  overflow-y: auto;
}
/*选中样式*/
#big_box .el-menu-item.is-active{
  background:#0096ff!important;
  box-sizing: border-box;
  color:#fff!important;
}
.side_nave_left{
  position: fixed;
  height: 100%;
}
.el-submenu .el-menu-item {
  height: 50px;
  line-height: 50px;
  padding: 0 45px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  box-sizing: border-box;
}
.el-submenu .el-menu-item:hover{
  color: #fff!important;
}
.iconimg{
  margin-right: 10px;
}
/*定义滚动条高宽及背景 高宽分别对应横竖滚动条的尺寸*/
::-webkit-scrollbar
{
  width: 8px;
  height: 8px;
  background-color: #F5F5F5;
}

/*定义滚动条轨道 内阴影+圆角*/
::-webkit-scrollbar-track
{
  -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
  border-radius: 10px;
  background-color: #F5F5F5;
}

/*定义滑块 内阴影+圆角*/
::-webkit-scrollbar-thumb
{
  border-radius: 10px;
  -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);
  background-color: #cccccc}
