/* 交易记录*/
.detailtable{
  width: 100%;
  border-collapse: collapse;
  font-size: 14px;
  table-layout: fixed;
}
.detailtable>tr{
  overflow: hidden;
}
.detailtable>tr>td{
  border: 1px solid #bbe4ff;
  padding: 10px 5px 20px 5px;
  line-height: 24px;
}
.detailtable>tr>td{
  width:20%;
}
.detailtable>tr>td>span{
  display: inline-block;
  width: 100%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  text-align: left;
}
.detailtable>tr>td:nth-child(odd){
  width: 8%;
  min-width: 200px;
  overflow: hidden;
  background: #e3f4ff;
  text-align: right;
  color: #999999;
}
