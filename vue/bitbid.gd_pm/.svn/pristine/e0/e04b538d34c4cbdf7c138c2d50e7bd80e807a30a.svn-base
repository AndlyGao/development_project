  list-style:none;/*清除小圆点*/
  padding:0px;
}
#app{
  height: 100%;
}
#big_box{
  width: 100%;
  height: 100%;
}
/*侧边样式*/
.left_con_box{
  position: fixed;
  left: 0;
  top:0;
