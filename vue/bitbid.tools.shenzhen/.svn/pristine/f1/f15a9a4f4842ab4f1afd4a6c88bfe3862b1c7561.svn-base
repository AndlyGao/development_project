body,p,h1,h2,h3,h4,h5,h6,ul,li,ol,dl,dt,dd,input,form{
  margin:0px;
  list-style:none;/*清除小圆点*/
  padding:0px;
}
html, body {
  height: 100%;
}
.el-row {
  height: 100%;
}
.el-menu {
  border-right: solid 0px transparent;
  list-style: none;
  position: relative;
  margin: 0;
  padding-left: 0;
}
/*头部样式*/
#header{
  display: inline-block;
  width: 87.5%;
  height: 40px;
  background:#fff;
  box-shadow: 0 0 10px rgba(0,0,0,0.2);
  position: fixed;
  right: 0;
  top: 0;
  z-index: 1;
}
.top_bigbox {
